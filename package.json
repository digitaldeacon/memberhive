{
  "name": "memberhive",
  "version": "2.0.0-beta.1",
  "license": "AGPL-3.0",
  "scripts": {
    "ng": "ng",
    "test": "ng test",
    "lint": "./node_modules/.bin/nx workspace-lint && ng lint",
    "lint:api": "cd api && ./vendor/bin/phpcs --extensions=php --standard=./vendor/yiisoft/yii2-coding-standards/Yii2/  ./controllers ./models ./helpers ./config ./mail ./migrations ./commands",
    "lint:api:fix": "cd api && ./vendor/bin/phpcbf --extensions=php --standard=./vendor/yiisoft/yii2-coding-standards/Yii2/  ./controllers ./models ./helpers ./config ./mail ./migrations ./commands",
    "e2e": "ng e2e",
    "start": "ng serve --proxy-config proxy.conf.json",
    "start:aot": "yarn start --aot",
    "start:hmr": "yarn start --hmr -e=hmr",
    "start:ssl": "yarn start:aot --ssl 1 --ssl-key \"/Applications/MAMP/ssl/localhost.key\" --ssl-cert \"/Applications/MAMP/ssl/localhost.cert\"",
    "build": "rm -rf dist/ && ng build --prod",
    "i18n:init": "ng xi18n --outputPath apps/web/src/i18n",
    "i18n:de": "ng xi18n --outputPath apps/web/src/i18n --locale de",
    "i18n:code": "ngx-extractor -i apps/web/src/**/*.ts -f xlf -o apps/web/src/i18n/messages.en.xlf",
    "affected:apps": "./node_modules/.bin/nx affected:apps",
    "affected:build": "./node_modules/.bin/nx affected:build",
    "affected:e2e": "./node_modules/.bin/nx affected:e2e",
    "format": "./node_modules/.bin/nx format:write",
    "format:write": "./node_modules/.bin/nx format:write",
    "format:check": "./node_modules/.bin/nx format:check",
    "update": "ng update @nrwl/workspace",
    "update:check": "ng update",
    "workspace-schematic": "./node_modules/.bin/nx workspace-schematic",
    "affected:dep-graph": "./node_modules/.bin/nx affected:dep-graph",
    "dep-graph": "./node_modules/.bin/nx dep-graph",
    "help": "./node_modules/.bin/nx help",
    "affected:test": "./node_modules/.bin/nx affected:test",
    "affected:lint": "./node_modules/.bin/nx affected:lint",
    "affected:libs": "./node_modules/.bin/nx affected:libs",
    "nx": "nx"
  },
  "private": true,
  "dependencies": {
    "@agm/core": "^1.0.0-beta.2",
    "@angular/animations": "~9.1.0",
    "@angular/cdk": "^7.0.0-beta.0",
    "@angular/common": "~9.1.0",
    "@angular/compiler": "~9.1.0",
    "@angular/core": "~9.1.0",
    "@angular/flex-layout": "^7.0.0-beta.23",
    "@angular/forms": "~9.1.0",
    "@angular/material": "^7.0.0-beta.0",
    "@angular/material-moment-adapter": "^7.0.0-beta.0",
    "@angular/platform-browser": "~9.1.0",
    "@angular/platform-browser-dynamic": "~9.1.0",
    "@angular/router": "~9.1.0",
    "@angular/service-worker": "~9.1.0",
    "@ngrx/effects": "8.3.0",
    "@ngrx/router-store": "8.3.0",
    "@ngrx/store": "8.3.0",
    "@ngx-translate/i18n-polyfill": "^1.0.0",
    "@nrwl/angular": "9.3.0",
    "@types/jwt-decode": "^2.2.1",
    "@types/lodash": "^4.14.67",
    "core-js": "^2.5.4",
    "dragula": "^3.7.2",
    "jwt-decode": "^2.2.0",
    "localforage": "^1.7.1",
    "lodash": "^4.17.10",
    "material-design-icons": "^3.0.1",
    "moment": "^2.22.1",
    "ng2-dragula": "^2.0.4",
    "ngx-img-cropper": "^0.11.0",
    "ngx-moment": "^3.0.0",
    "roboto-fontface": "^0.10.0",
    "rxjs": "^6.5.5",
    "rxjs-compat": "^6.3.2",
    "tinymce": "^4.7.13",
    "tslib": "^1.9.0",
    "zone.js": "^0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.0",
    "@angular/cli": "^9.1.6",
    "@angular/compiler-cli": "~9.1.0",
    "@angular/language-service": "~9.1.0",
    "@ngrx/schematics": "8.3.0",
    "@ngrx/store-devtools": "8.3.0",
    "@nrwl/workspace": "9.3.0",
    "@types/jasmine": "~3.3.5",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~10.12.18",
    "codelyzer": "~4.5.0",
    "dotenv": "6.2.0",
    "git-describe": "^4.0.2",
    "jasmine-core": "~3.3.0",
    "jasmine-marbles": "0.4.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.1.4",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.4",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "prettier": "^2.0.5",
    "protractor": "~5.4.0",
    "rxjs-tslint-rules": "^4.8.0",
    "ts-node": "~7.0.1",
    "tslint": "6.0.0",
    "typescript": "~3.8.3"
  }
}
